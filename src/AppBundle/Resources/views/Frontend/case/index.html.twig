{% extends '@Layouts/frontendFrame.html.twig' %}
{% from '@Frontend/Macros/caselist.html.twig' import caselist %}
{% from '@Frontend/partial/toolbar.html.twig' import toolbar %}
{% block title %}
  {% set _route = app.request.attributes.get('_route') %}
  {% if _route =='case_list' %}
  <title>设计案例 - 网站建设|北京网站制作|北京网站设计</title>
  <meta name="keywords" content="案例详情">
  <meta name="description" content="案例详情">
  {% elseif _route == 'case_category' %}
    <title>{{ current.name }}行业网站建设案例 - 北京网站建设|北京网站制作|北京网站设计</title>
    <meta name="keywords" content="{{ current.keywords }}">
    <meta name="description" content="{{ current.description }}">
  {% endif %}
{% endblock %}
{% block content %}
  {% include '@Frontend/partial/inner_header.html.twig' %}
  {{ toolbar(categorys,2) }}
  {{ caselist(cases) }}
{% endblock %}

{% block css %}
<link href="{{ asset('static/css/base.css','frontend') }}" rel="stylesheet">
{% endblock %}
{% block js %}
<script src="{{ asset('static/js/inner.js') }}"></script>
{% endblock %}